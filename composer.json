{
    "name": "librarymarket/msadiag",
    "description": "The Message Submission Agent Diagnostics tool (msadiag) facilitates testing the compatibility of third party message submission agents.",
    "homepage": "https://github.com/librarymarket/msadiag",
    "keywords": ["agent", "diagnostics", "esmtp", "mail", "message", "smtp", "submission", "test", "tool"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Clay Freeman",
            "email": "clay@librarymarket.com",
            "homepage": "https://www.librarymarket.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["msadiag"],
    "config": {
        "sort-packages": true,
        "discard-changes": true
    },
    "autoload": {
        "psr-4": {
            "LibraryMarket\\msadiag\\": "src"
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/ca-bundle": "^1.3",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.6"
    }
}
